local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/ShilohFloof/Solaris-Lib/main/solaris"))()

local win = SolarisLib:New({
  Name = "Blox Fruit Hub Archive",
  FolderToSave = "SolarisLibStuff"
})


local tab = win:Tab("No Key Hubs")
local tab2 = win:Tab("Key Required Hubs")
local sec = tab:Section("Working Hubs")
local sec2 = tab2:Section("Working Hubs")

--No Key Hubs
sec:Button("Makima Hub",function() loadstring(game:HttpGet('https://raw.githubusercontent.com/Piwwy0909/MakimaRemake/main/Cumback.Makima'))()
  SolarisLib:Notification("Script Loaded!", "Makima Hub has been loaded.")
end)

